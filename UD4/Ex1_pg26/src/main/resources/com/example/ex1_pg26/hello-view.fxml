<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>
<BorderPane xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.ex1_pg26.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <left>
        <StackPane fx:id="leftPane" alignment="CENTER">
            <Button fx:id="b1" >B1</Button>
        </StackPane>
    </left>
    <center>
        <StackPane fx:id="centerPane" alignment="CENTER">
            <Button fx:id="b2">B2</Button>
        </StackPane>
    </center>
    <right>
        <StackPane fx:id="rightPane" alignment="CENTER">
            <Button fx:id="b3">B3</Button>
        </StackPane>
    </right>
<bottom>
    <Label fx:id="eventCheck"/>
</bottom>
</BorderPane>
